[gpg]
  program = "gpg"
[user]
  signingkey = "{{ .githubemail }}"
  email = "{{ .githubemail }}"
  name = "{{ .githubusername }}"
[commit]
  gpgsign = true
[core]
  editor = "vim"
[credential]
  helper = cache --timeout=43200
[push]
  default = simple
[alias]
  hist = log --graph --pretty=format:'%C(yellow)%h%C(cyan)%d%Creset %s %C(white)- %an, %ar%Creset'
  co = checkout
  ci = "commit -S -m "
  cob = "checkout -b "
  st = status
  br = branch
  type = "cat-file -t "
  dump = "cat-file -p "
[init]
  defaultBranch = main
